{
  "version": 2,
  "headers": [
    {
      "source": "/((?!.*\\.).*)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    },
    {
      "source": "/lists/vintagebus/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/([^\\.]+)$",
      "destination": "/api/serve-no-ext"
    },
    {
      "source": "/bartnik$",
      "destination": "/~bartnik/"
    },
    {
      "source": "/m-codes$",
      "destination": "/~molenaar/m-codes/"
    },
    {
      "source": "/molenaar$",
      "destination": "/~molenaar"
    },
    {
      "source": "/rvanness$",
      "destination": "/~rvanness/"
    },
    {
      "source": "/rescue$",
      "destination": "/~airsworks/rescue/"
    },
    {
      "source": "/~airswork$",
      "destination": "/~airswork/"
    },
    {
      "source": "/~bartnik$",
      "destination": "/~bartnik/"
    },
    {
      "source": "/~keen$",
      "destination": "/~keen/"
    },
    {
      "source": "/~molenaar$",
      "destination": "/~molenaar/"
    },
    {
      "source": "/~rvanness$",
      "destination": "/~rvanness/"
    }
  ]
}
