{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static",
      "config": {
        "buildCommand": "mkdir -p .vercel/output/static && rsync -av --exclude .git --exclude .vercel --exclude node_modules . .vercel/output/static"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/([^\\.]+)$",
      "destination": "/api/serve-no-ext"
    },
    {
      "source": "/bartnik$",
      "destination": "/~bartnik/"
    },
    {
      "source": "/m-codes$",
      "destination": "/~molenaar/m-codes/"
    },
    {
      "source": "/molenaar$",
      "destination": "/~molenaar/"
    },
    {
      "source": "/rvanness$",
      "destination": "/~rvanness/"
    },
    {
      "source": "/rescue$",
      "destination": "/~airsworks/rescue/"
    },
    {
      "source": "/~airswork$",
      "destination": "/~airswork/"
    },
    {
      "source": "/~bartnik$",
      "destination": "/~bartnik/"
    },
    {
      "source": "/~keen$",
      "destination": "/~keen/"
    },
    {
      "source": "/~molenaar$",
      "destination": "/~molenaar/"
    },
    {
      "source": "/~rvanness$",
      "destination": "/~rvanness/"
    }
  ]
}
