<script>
  $(document).ready(function() {
	  
	  $('#formAlert').slideUp(100);
	  $('#cselect').flagStrap();
	  if ( $('#alerttext').text().trim() != '' ) {
		  $('#formAlert').slideDown(400);
	  }
	  $('#submit').click(function() {
		if ( $('#inputEmail').val() != $('#inputEmailConf').val() ) {
			$('#alerttext').text('Your email addresses do not match, please correct.');
			$('#formAlert').slideDown(400);
			$('html,body').animate({
				scrollTop: $('#formAlert').offset().top -80 }, 400);
			return;
		}
		if ( $('#inputPassword').val() != $('#inputPasswordConf').val() ) {
			$('#alerttext').text('Your passwords do not match, please correct.');
			$('#formAlert').slideDown(400);
			$('html,body').animate({
				scrollTop: $('#formAlert').offset().top - 80 }, 400);
			return;
		}
		if ( $('#inputPassword').val().trim() == '' ) {
		        $('#alerttext').text('Your password cannot be blank, please input a password.');
			$('#formAlert').slideDown(400);
			$('html,body').animate({
				scrollTop: $('#formAlert').offset().top - 80 }, 400);
			return;
	        }
		if ( $('#inputEmail').val().trim() == '' ) {
	  		$('#alerttext').text('Your email address cannot be blank, please input your email address.');
			$('#formAlert').slideDown(400);
			$('html,body').animate({
				scrollTop: $('#formAlert').offset().top - 80 }, 400);
			return;
                }
		$('form #register').submit();

	  });

  });
</script>

